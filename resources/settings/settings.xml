    <setting propertyKey = "@Properties.field-5" title = "@Strings.field_5_title">
    	<settingConfig type = "list">
    		<listEntry value = "0">@Strings.DisplayExtraTz</listEntry>
    		<listEntry value = "1">@Strings.DisplayExchangeRate</listEntry>
    		<listEntry value = "2">@Strings.DisplayDistance</listEntry>
    		<listEntry value = "3">@Strings.DisplayPulse</listEntry>
    		<listEntry value = "4">@Strings.DisplayFloors</listEntry>
    		<listEntry value = "7">@Strings.DisplayAltitude</listEntry>
    		<listEntry value = "8">@Strings.DisplayCalories</listEntry>
