<settings>

    <setting propertyKey="@Properties.appVersion" title="@Strings.AppVersionTitle">
        <settingConfig type="alphaNumeric" readonly="true" /> 
    </setting>
   
    <setting propertyKey="@Properties.is-show-seconds" title="@Strings.isShowSecondsTitle">
    	<settingConfig type="boolean"></settingConfig>
    </setting>
   
    <setting propertyKey="@Properties.BackgroundColor" title="@Strings.BackgroundColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.TimeColor" title="@Strings.TimeColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <listEntry value="0x555555">@Strings.ColorDarkGray</listEntry>
            <listEntry value="0xAAAAAA">@Strings.ColorLightGray</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>            
            <listEntry value="0x0000FF">@Strings.ColorBlue</listEntry>
            <listEntry value="0xFF0000">@Strings.ColorRed</listEntry>
            <listEntry value="0x32CD32">@Strings.ColorLimeGreen</listEntry>
            <listEntry value="0x00FF00">@Strings.ColorLime</listEntry>
            <listEntry value="0xD2FF0C">@Strings.ColorYellowGreen</listEntry>
            <listEntry value="0xC8FFBF">@Strings.ColorPaleGreen</listEntry>
            <listEntry value="0xB5EBFF">@Strings.ColorPaleBlue</listEntry>
            <listEntry value="0xFFD3D9">@Strings.ColorPink</listEntry>
            <listEntry value="0x00F6FF">@Strings.ColorAqua</listEntry>
            <listEntry value="0xFFFF00">@Strings.ColorYellow</listEntry>
            <listEntry value="0xFF422D">@Strings.ColorYellowRed</listEntry>
            <listEntry value="0xF88E46">@Strings.ColorOrange</listEntry>
        </settingConfig>
    </setting>
    
    <setting propertyKey="@Properties.BrightColor" title="@Strings.BrightColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <listEntry value="0x555555">@Strings.ColorDarkGray</listEntry>
            <listEntry value="0xAAAAAA">@Strings.ColorLightGray</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.DimColor" title="@Strings.DimColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <listEntry value="0x555555">@Strings.ColorDarkGray</listEntry>
            <listEntry value="0xAAAAAA">@Strings.ColorLightGray</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>            
        </settingConfig>
    </setting>
    

    
    <setting propertyKey="@Properties.is-show-weather" title="@Strings.isShowWeatherTitle">
    	<settingConfig type="boolean"></settingConfig>
    </setting>
    
    <setting propertyKey="@Properties.weather-provider" title="@Strings.weatherProviderTitle">
     	<settingConfig type="list">
    		<listEntry value="0">@Strings.openWeatherTitle</listEntry>
    		<listEntry value="1">@Strings.darkSkyTitle</listEntry>
    	</settingConfig>   
    </setting>
    
   	<!-- <setting propertyKey="@Properties.WeatherApiKey" title="@Strings.WeatherApiKeyTitle">
    	<settingConfig type="alphaNumeric"></settingConfig>
    </setting> -->
    
    <setting propertyKey="@Properties.tempSystem" title="@Strings.TempSystemTitle">
    	<settingConfig type="list">
    		<listEntry value="1">@Strings.Celsius</listEntry>
    		<listEntry value="2">@Strings.Farenheit</listEntry>
    	</settingConfig>
    </setting>
    
    <setting propertyKey="@Properties.windSystem" title="@Strings.WindSystemTitle">
    	<settingConfig type="list">
    		<listEntry value="0">@Strings.KilometersPh</listEntry>
    		<listEntry value="1">@Strings.Knots</listEntry>
    		<listEntry value="2">@Strings.Meters</listEntry>
    		<listEntry value="3">@Strings.MilesPh</listEntry>
    	</settingConfig>
    </setting> 
    
   	<setting propertyKey="@Properties.is-show-city" title="@Strings.IsShowCityTitle">
    	<settingConfig type="boolean"></settingConfig>
    </setting>
    
    <setting propertyKey="@Properties.city-align" title="@Strings.CityAlignTitle">
    	<settingConfig type="list">
    		<listEntry value="0">@Strings.Left</listEntry>
    		<listEntry value="1">@Strings.Center</listEntry>
    	</settingConfig>
    </setting>
    
    <setting propertyKey="@Properties.show-alarm" title="@Strings.IsShowAlarmTitle">
    	<settingConfig type = "list">
    		<listEntry value = "0">@Strings.ShowNever</listEntry>
    		<listEntry value = "1">@Strings.ShowOnlyValue</listEntry>
    		<listEntry value = "2">@Strings.ShowAlways</listEntry>
    	</settingConfig>
    </setting>
    
    <setting propertyKey="@Properties.show-message" title="@Strings.IsShowMessageTitle">
    	<settingConfig type = "list">
    		<listEntry value = "0">@Strings.ShowNever</listEntry>
    		<listEntry value = "1">@Strings.ShowOnlyValue</listEntry>
    		<listEntry value = "2">@Strings.ShowAlways</listEntry>
    	</settingConfig>
    </setting>
 

       
